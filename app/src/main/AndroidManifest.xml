<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adityaagusw.examplelaravelcrudmvp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Pagination.PaginationActivity"></activity>
        <activity android:name=".Activity.Search.SearchActivity" />
        <activity android:name=".Activity.Lihat.Mahasiswa.LihatMhsActivity" />
        <activity android:name=".Activity.Lihat.Note.LihatNoteActivity" />
        <activity android:name=".Activity.Mahasiswa.InputMhsActivity" />
        <activity android:name=".Activity.Note.InputNoteActivity" />
        <activity android:name=".Activity.Auth.Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Auth.Register.RegisterActivity" />
        <activity android:name=".Activity.Main.MainActivity" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
    </application>

</manifest>